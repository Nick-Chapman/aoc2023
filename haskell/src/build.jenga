
executable-name:
  echo aoc.exe > $@

include ../../../jenga/rulelib/haskell.jenga

main-entry:
  echo Advent2023 > $@

ghc-version:
  echo 9.8.4 > $@

ghc-options:
  echo -Wall >> $@
  echo -Wno-name-shadowing >> $@
  echo -Wno-x-partial >> $@ #head/tail
  echo -Werror >> $@

default-extensions:
  echo DuplicateRecordFields >> $@
  echo GADTs >> $@
  echo LambdaCase >> $@
  echo MultiWayIf >> $@

packages:
  echo extra >> $@
  echo unordered-containers >> $@
